public class ExampleForMap {

	public static void main(String[] args) {
		LinkedForHashMap lhm=new LinkedForHashMap();
		lhm.put("name:",anand);
		lhm.put("age",20);
		System.out.println(lhm);
		System.out.println("--------------------");
		System.out.println(lhm.get("name"));
		for(Object i:lhm.keySet(0)) {
			System.out.println(i);
		}
		System.out.println("--------------------");
		for(Object i:lhm.entrySet());
		System.out.println(i);

	}

}
